<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.SboardVO">

	<select id="selectList" resultType="kr.or.ddit.vo.SboardVO">
		SELECT 
		    BO_NO
		    , BO_TITLE
		    , BO_WRITER
		    , BO_CONTENT
		    , BO_DATE
		    , BO_HIT
		    , FILE_GROUP_NO
		FROM SBOARD
	</select>




</mapper>